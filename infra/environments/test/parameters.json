{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environment": {
      "value": "test"
    },
    "envName": {
      "value": "engram-test"
    },
    "location": {
      "value": "[resourceGroup().location]"
    },
    "aksNodeCount": {
      "value": 3
    },
    "aksNodeSize": {
      "value": "Standard_D2s_v3"
    },
    "aksNodeMinCount": {
      "value": 3
    },
    "aksNodeMaxCount": {
      "value": 6
    },
    "postgresSku": {
      "value": "D2s_v3"
    },
    "postgresStorageGB": {
      "value": 64
    },
    "enablePrivateLink": {
      "value": false
    },
    "blobStorageRedundancy": {
      "value": "ZRS"
    },
    "temporalHistoryShards": {
      "value": 8
    },
    "enableCodecServer": {
      "value": false
    },
    "logRetentionDays": {
      "value": 30
    }
  }
}

